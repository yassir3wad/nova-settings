{
    "name": "yassir3wad/nova-settings",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Yassir3wad\\Settings\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yassir3wad\\Settings\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
